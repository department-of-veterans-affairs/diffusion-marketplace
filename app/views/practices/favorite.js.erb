<% if @favorited %>
  $('.dm-favorite-icon-<%= @practice.id %>').removeClass('far');
  $('.dm-favorite-icon-<%= @practice.id %>').addClass('fas');
  $('.dm-favorite-practice-link-text').text('Bookmarked');
<% else %>
  $('.dm-favorite-icon-<%= @practice.id %>').removeClass('fas');
  $('.dm-favorite-icon-<%= @practice.id %>').addClass('far');
  $('.dm-favorite-practice-link-text').text('Bookmark');
<% end %>
