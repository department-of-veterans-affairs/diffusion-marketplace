<div class="usa-combo-box margin-y-0" data-enhanced="true">
  <select id="<%= facility_category_selector %>" class="usa-select" name="<%= facility_category_selector %>">
    <option value>All categories</option>
    <% @categories.each do |c| %>
      <option value=<%= c.id %>>
        <%= c.name %>
      </option>
    <% end %>
  </select>
</div>