<label for="<%= facility_type_selector %>" class="usa-label line-height-26">Complexity</label>
<div class="margin-top-1 margin-bottom-2">
  <select id="<%= facility_type_selector %>" class="usa-select" name="<%= facility_type_selector %>">
    <option value="">- Select -</option>
    <% @types.each do |type| %>
      <option value=<%= type %>
          <% if params[:type].present? && type.include?(params[:type].to_s) %>
            selected
          <% end %>
       >
          <%= type %>
        </option>
    <% end %>
  </select>
</div>