<% @practice.additional_documents.each do |ad| %>
  <div class="display-flex flex-justify margin-bottom-2 hyphens-auto word-break-break-word">
    <%= link_to ad.attachment_s3_presigned_url, class: 'additional-document-link font-sans-xs-2 line-height-sans-505', download: ad.attachment_file_name do %>
      <%= ad.title %>
    <% end %>
  </div>
<% end %>