<div class="usa-alert usa-alert--info usa-alert--no-icon margin-bottom-1 maturity-level-banner">
  <div class="usa-alert__body line-height-sans-6">
    <%
      maturity = @practice.maturity_level
    %>
    <p class="usa-alert__text x0-bottom">
      <%= Practice::MATURITY_LEVEL_MAP[maturity.to_sym][:description] %>
      <a href="/search?query=<%= maturity %>" class="dm-internal-link">See more <%= maturity %> practices.</a>
    </p>
  </div>
</div>