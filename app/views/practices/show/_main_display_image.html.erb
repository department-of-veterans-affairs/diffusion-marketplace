<% if @practice.main_display_image.present? %>
  <% main_image_alt_text = @practice.main_display_image_alt_text %>
  <img
    class="dm-thumbnail-image display-block"
    src="<%= @practice.main_display_image_s3_presigned_url(:thumb) %>"
    alt="<%= main_image_alt_text.present? ? main_image_alt_text : "#{@practice.name} main image" %>"/>
<% end %>