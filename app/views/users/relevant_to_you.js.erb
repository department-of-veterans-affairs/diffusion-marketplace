$('.favorite-practices').append("<%= escape_javascript render(partial: 'practices/show/marketplace_cards_container', locals: {
        container_classes: 'margin-bottom-105',
        container_header_classes: nil,
        container_title: nil,
        practice_data_set: @paginated_favorite_practices
}) %>")
<% link = pagy_link_proc(@pagy_favorite_practices) %>
$('.favorite-practices-page-1').remove();
<% if @pagy_favorite_practices.next.present? %>
  $(".favorite-practices-page-<%= @pagy_favorite_practices.prev %>").remove();
  $('.favorite-practices').append(`<%= link.call(@pagy_favorite_practices.next, 'Load more').html_safe %>`);
<% else %>
  $(".favorite-practices-page-<%= @pagy_favorite_practices.prev %>").remove();
  $(".favorite-practices").last().css('margin-bottom', '48px');
<% end %>